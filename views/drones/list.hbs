<h2>Available drones</h2>

<div class="row">
{{#each drones}}
<div class="card" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title">{{this.name}}</h5>
    <p class="card-text">Propellers: {{this.propellers}}</p>
    <p class="card-text">Max Speed: {{this.maxSpeed}}</p>
    <a href="#" class="btn btn-primary">Go somewhere</a>
  </div>
</div>
{{/each}}
</div>